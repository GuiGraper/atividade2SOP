pr2 = ["A", "B", "B", "B", "a"]
prc2 = ["A", "B", "B", "B"]

if tempo1 <= tempo2: 
  
  for i in range(t1): 
    p = pr1[i]
    print(" ") 
    print("Processo em inicio", p, "O processo pendente é:", prc1)
    t = len(prc1)
    print("Os processos prontos são:", e1)
