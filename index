print("FIFO")
e1 = []
e2 = []

p1 = ["A", "a","b", "B", "b"]
pr1 =["A","a", "b", "B"]
pra1 =["A", "B","C", "D", "E"]
p2 = ["A", "b", "a", "A", "b"]
pr2 = ["A", "b", "a", "A" ]

for i in range(len(p1)):
  p = p1[i] #A
  print(" ") 
  print("Processo em inicio", p, "O processo pendente é:", pr1)
  t = len(pr1)


  if t >= 1:
    del pr1[0]
else
  if i >= 0:
    print("Os processos prontos são", e1)
  e1.append(p)


  
print(" ")


for i in range(len(p2)):
  p = p2[i]
  print(" ")
  print("Processo em inicio", p, "O processo pendente é:", pr2)
  t = len(pr2)
  if i >= 0:
    print("O processo foi:", e2)
  e2.append(p)
  if t >= 1:
    del pr2[0]


print("")
print("SJF")
print("")
