Processo1 = ["A", "a", "B", "b", "A"]
Processo2 = ["a", "A", "b", "A"]
e1.clear()
e2.clear()

print("Iniciando processo com prioridade 1")

if pri1[1][0] == "P1":
  for i in range(len(pri1[0])):
  
    p = pri1[0][i]
    print(" ") 
    print("Processo em inicio", p, "O processo pendente é:", pric1)
    t = len(pric1)
    if t >= 1:
      del pric1[0] 
//else
    if i >= 1:
      print("Os processos prontos são", e1)
    e1.append(p)
  
  print(" ")
  print("Iniciar processo com processo 2")
  for i in range(len(pri2[0])):
    p = pri2[0][i]
    print(" ") 
    print("Processo em inicio", p, "O processo pendente é:", pric2)
    t = len(pric2)
    if t >= 1:
      del pric2[0]
    if i >= 1:
      print("Os processos prontos são", e2)
    e2.append(p)


else:
  print("Iniciar processo junto ao 2")
  for i in range(len(pri2[0])):
      p = pri2[0][i]
      print(" ") 
      print("Processo em inicio", p, "O processo pendente é:", pric2)
      t = len(pric2)
      if t >= 1:
        del pric2[0]
      if i >= 1:
        print("Os processos prontos são", e2)
      e2.append(p)

  
  print(" ")
  print("Iniciar processo junto ao 1")
  for i in range(len(pri1[0])):
      p = pri1[0][i]
      print(" ") 
      print("Processo em inicio", p, "O processo pendente é:", pric1)
      t = len(pric1)
      if t >= 1:
        del pric1[0] #
      if i >= 1:
